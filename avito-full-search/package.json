{
  "name": "avito-scanner",
  "engines": {
    "node": "14"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/lodash": "^4.14.195",
    "@types/node": "^14.18.53",
    "@types/request": "^2.48.8",
    "gts": "^3.1.1",
    "request": "^2.88.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "exec": "ts-node -P ./tsconfig.json --files -r tsconfig-paths/register",
