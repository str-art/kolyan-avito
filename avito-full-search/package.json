    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "dayjs": "^1.11.9",
    "exponential-backoff": "^3.1.1",
    "lodash": "^4.17.21",
    "node-html-parser": "^6.1.5",
    "tor-request": "^3.1.0"
  }
